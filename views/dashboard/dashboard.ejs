<%- include('../client/layouts/header') %>

<!-- Page Header-->
<header class="masthead"
        style="background-image: url('https://cdn.pixabay.com/photo/2018/07/14/15/27/cafe-3537801_1280.jpg')">
    <div class="container position-relative px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-md-10 col-lg-8 col-xl-7">
                <div class="page-heading">
                    <h1>Dashboard</h1>
                </div>
            </div>
        </div>
    </div>
</header>

<div class="container">
        <% dashboardActions.forEach((d)=>{ %>
    <div class="row">
            <button id="<%= d.actionId %>" onclick="catchID(this.id)" class="btn btn-outline-primary m-1">
                <%= d.displayName %>
            </button>

    </div>

    <% }); %>

</div>
<script>
    function catchID(clickedId){
alert(clickedId + "butona tıklandıs")
    }
</script>
<%- include('../client/layouts/footer') %>